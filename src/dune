
(executable
 (name wombat)
 (libraries Vmu Compiler core ANSITerminal)
 (preprocess (pps ppx_let))
 ; (flags (-ccopt -static))
 (modules Wombat))

(executable
 (name       mytoplevel)
 (libraries  Vmu core ANSITerminal compiler-libs.toplevel)
 (link_flags (-linkall))
 (modules mytoplevel)
 (modes      byte))

(install
 (section bin)
 (files (wombat.exe as wombat)
        (mytoplevel.exe as mytoplevel)))
